import java.util.*;
class SunnyNumber{
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int n=s.nextInt();
        if(Math.sqrt(n+1)%1==0){
            System.out.println(n+" is Sunny Number");
        }
        else
            System.out.println(n+" is not Sunny Number");
    }

}
