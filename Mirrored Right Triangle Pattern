import java.util.*;
class MirroredRightTrianglePattern {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int r=s.nextInt(),space=r;
        for(int i=0;i<r;i++){
            for(int j=0;j<space;j++){
                System.out.print(" ");
            }
               space--;
            for(int j=0;j<=i;j++){
                System.out.print("*");
            }
            System.out.println();
        }
    }
}
