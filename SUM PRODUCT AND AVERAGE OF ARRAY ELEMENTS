public class Example {
    public static void main(String[] args) {
        int[] n={1,2,3,4,5};
        int sum=0;int product=1;
        double average;
        for(int i=0;i<n.length;i++){
            sum += n[i];
            product *= n[i];
        }
        average = (double)sum/n.length;
        System.out.println("sum:"+sum+" "+"product:"+product+" "+"average :"+average);
    }
}
