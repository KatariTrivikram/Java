import java.util.Scanner;

public class Sum {
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n = s.nextInt();
        int sum = 0;
        int i;
        while(n > 0){
            i= n%10;
            n = n/10;
            sum+=i;
        }
        System.out.println("sum\t"+sum);

    }

}
