import java.util.*;
class Pyramid {
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int r=s.nextInt();
        for(int i=0;i<r;i++){
            for(int j=r;j>i+1;j--) {
                System.out.print(" ");
            }
            for(int l=0;l<=i;l++){
                System.out.print("* ");
            }


            System.out.println();
        }

    }
}
